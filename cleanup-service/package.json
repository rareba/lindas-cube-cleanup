{
  "name": "lindas-cube-cleanup-service",
  "version": "1.0.0",
  "description": "LINDAS Cube Version Cleanup Service - supports Fuseki, Stardog, and GraphDB",
  "main": "src/index.js",
  "bin": {
    "lindas-cleanup": "./src/cli.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "cleanup": "node src/cli.js cleanup",
    "restore": "node src/cli.js restore",
    "list-backups": "node src/cli.js list-backups",
    "preview": "node src/cli.js cleanup --dry-run",
    "test": "node --test src/**/*.test.js"
  },
  "keywords": [
    "lindas",
    "sparql",
    "rdf",
    "cube",
    "cleanup",
    "fuseki",
    "stardog",
    "graphdb"
  ],
  "author": "Giulio V",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "node-fetch": "^2.7.0",
    "@aws-sdk/client-s3": "^3.400.0",
    "@aws-sdk/lib-storage": "^3.400.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
